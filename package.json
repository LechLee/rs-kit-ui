{
  "name": "has-ui-kit-v4",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@8.15.4",
  "scripts": {
    "build": "pnpm -r build",
    "build:ui-kit": "pnpm --filter ui-kit build",
    "playground": "pnpm --filter playground dev",
    "start": "pnpm build:ui-kit && (pnpm --filter ui-kit dev & sleep 2 && pnpm --filter playground dev)"
  },
  "devDependencies": {
    "@rspack/cli": "^0.5.1",
    "@rspack/core": "^0.5.1",
    "typescript": "^5.2.2"
  }
}
